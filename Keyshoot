-- Drag System
local dragging = false
local dragStart, startPos

mainFrame.InputBegan:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 then
		dragging = true
		dragStart = input.Position
		startPos = mainFrame.Position
		input.Changed:Connect(function()
			if input.UserInputState == Enum.UserInputState.End then
				dragging = false
			end
		end)
	end
end)

UserInputService.InputChanged:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseMovement and dragging then
		local delta = input.Position - dragStart
		mainFrame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X,
			startPos.Y.Scale, startPos.Y.Offset + delta.Y)
	end
end)

-- Events
closeBtn.MouseButton1Click:Connect(function()
	TweenService:Create(mainFrame, TweenInfo.new(0.4), {BackgroundTransparency = 1}):Play()
	task.wait(0.4)
	keyGui:Destroy()
	game.StarterGui:SetCore("SendNotification", {
		Title = "Key System",
		Text = "‚ùå ‡∏¢‡∏Å‡πÄ‡∏•‡∏¥‡∏Å‡πÅ‡∏•‡πâ‡∏ß",
		Duration = 3
	})
end)

submitBtn.MouseButton1Click:Connect(function()
	local keyInput = keyBox.Text
	local isValid = false

	local success, data = pcall(function()
		return loadstring(game:HttpGet(keysURL))()
	end)

	if success and data[keyInput] == true then
		TweenService:Create(mainFrame, TweenInfo.new(0.5), {BackgroundTransparency = 1}):Play()
		task.wait(0.5)
		keyGui:Destroy()
		game.StarterGui:SetCore("SendNotification", {
			Title = "Key System",
			Text = "‚úÖ Key ‡∏ñ‡∏π‡∏Å‡∏ï‡πâ‡∏≠‡∏á!",
			Duration = 3
		})
		successSound:Play()
		loadstring(game:HttpGet(scriptURL, true))()
	else
		keyBox.Text = ""
		keyBox.PlaceholderText = "‚ùå Key ‡∏ú‡∏¥‡∏î!"
		errorSound:Play()
	end
end)

getKeyBtn.MouseButton1Click:Connect(function()
	setclipboard(keySiteURL)
	game.StarterGui:SetCore("SendNotification", {
		Title = "Get Key",
		Text = "üìã ‡∏•‡∏¥‡∏á‡∏Å‡πå‡∏Ñ‡∏±‡∏î‡∏•‡∏≠‡∏Å‡πÅ‡∏•‡πâ‡∏ß!",
		Duration = 3
	})
end)

