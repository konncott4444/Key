RunService.RenderStepped:Connect(function()
	local newY = math.sin(tick() * 5) * 5 + 15
	logo.Position = UDim2.new(0, 15, 0, newY)
end)

TweenService:Create(loadingFrame, TweenInfo.new(0.8), {BackgroundTransparency = 0.05}):Play()
